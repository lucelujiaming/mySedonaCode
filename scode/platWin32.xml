<!--
  -  Sedona SCode manifest for a win32 platform
  -->

<sedonaCode
   endian="little"
   blockSize="4"
   refSize="4"
   main="sys::Sys.main"
   debug="false"
   test="false"
>

  <!-- Kits -->
  <depend on="sys 1.2"           test="true" />
  <depend on="sox 1.2"           test="true" />
  <depend on="inet 1.2"          test="true" />
  <depend on="web 1.2"           test="true" />

  <!-- Deprecated as of 1.2
  <depend on="control 1.0+"       test="true" />
  -->
  <depend on="pstore 1.2"        test="true" />
  <depend on="datetime 1.2"      test="true" />
  <depend on="datetimeStd 1.2"   test="true" />
  <depend on="basicSchedule 1.2" test="true" />
  <depend on="platWin32 1.2"     test="true" />

  <!-- These replace control kit as of 1.2 -->
  <depend on="func 1.2"          test="true" />
  <depend on="hvac 1.2"          test="true" />
  <depend on="logic 1.2"         test="true" />
  <depend on="math 1.2"          test="true" />
  <depend on="timing 1.2"        test="true" />
  <depend on="types 1.2"         test="true" />
  <depend on="pricomp 1.2"       test="true" />

</sedonaCode>

