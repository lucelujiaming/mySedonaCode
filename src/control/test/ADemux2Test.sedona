**
** ADemux2Test
**
class ADemux2Test
  extends Test
{
  static inline ADemux2  demux

 
  static void test()
  {
    // Test bool
    demux.in := 23f
    assert(demux.out1 == 0f)
    assert(demux.out2 == 0f)
    
    demux.execute()
    assert(demux.out1 == 23f)
    assert(demux.out2 == 0f)

    demux.in := -1.43f
    demux.s1 := true
    assert(demux.out1 == 23f)
    assert(demux.out2 == 0f)
    
    demux.execute()
    assert(demux.out1 == 23f)
    assert(demux.out2 == -1.43f)

  }
}
