  **
  ** Execute the comparison
  **
  virtual override void execute()
  {
    if(enableCount >= 100)
       return 
    setAllCompareValues()
    float outCompare = sampleValue[0];
    for(int i = 1; i < enableCount; i++)
    {
      if(outCompare > sampleValue[i])
        outCompare = sampleValue[i]
    }
    out := outCompare
  }

